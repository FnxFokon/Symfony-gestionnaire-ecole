<form method="post" action="{{ path('app_teacher_delete', {'id': user.id}) }}" onsubmit="return confirm('Es tu certain de vouloir supprimer ce professeur ?');">
	<input type="hidden" name="_token" value="{{ csrf_token('delete' ~ user.id) }}">
	<button class="btn btn-danger m-2">Supprimer</button>
</form>
